@model CTL.Models.RegisterViewModel


<style type="text/css">
    input.big {
        height: 2.1em;
        width: 2.1em;
    }
</style>

@Html.Hidden("StepID", 1)
@Html.Hidden("UserIdentity", (string)ViewBag.UserId)
@Html.Hidden("StateStaffCheck")
@Html.Hidden("AgencySiteAccess")
@Html.Hidden("Coordinator")
<div align="center">

    <div class="w3-card-2" style="padding-left:30px;width:94%;text-align:left">
        <br />

        @if (!Request.IsAuthenticated)
        {

            <div style="display:block" id="UserSection">

                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">

                            <h3 style="font-family: 'Roboto', sans-serif;color:#666"><span class="glyphicon glyphicon-user"></span>&nbsp;Personal Information</h3>

                        </div>
                    </div>
                </div>

                <hr />
                <div class="row">
                    <div class="col-md-3">

                        <div>
                            <label style="padding-right:35px" for="FirstName">First Name</label>
                            @Html.TextBoxFor(m => m.FirstName, new { id = "FirstName", @class = "form-control", placeholder = "First Name" })
                            @Html.ValidationMessageFor(m => m.FirstName)
                        </div>

                    </div>

                    <div class="col-md-3">

                        <div>
                            <label style="padding-right:35px" for="LastName">Last Name</label>
                            @Html.TextBoxFor(m => m.LastName, new { id = "LastName", @class = "form-control", placeholder = "Last Name" })
                            @Html.ValidationMessageFor(m => m.LastName)
                        </div>

                    </div>

                    <div class="col-md-3">

                        <div>
                            <label style="padding-right:35px" for="LastName">Middle Name</label>
                            @Html.TextBoxFor(m => m.MiddleName, new { id = "MiddleName", @class = "form-control", placeholder = "Middle Name" })
                            @Html.ValidationMessageFor(m => m.MiddleName)
                        </div>

                    </div>


                </div>

                <br />
                <div class="row">
                    <div class="col-md-6">

                        <div>
                            <label style="padding-right:6px" for="UserName">Email address</label>
                            @Html.TextBoxFor(m => m.Email, new { id = "Email", @class = "form-control", placeholder = "Email Address (This will be your username)" })
                        </div>

                    </div>

                    <div class="col-md-3">

                        <div>
                            <label style="padding-right:35px" for="Telephone Number">Telephone Number</label>
                            @Html.TextBoxFor(m => m.TelephoneNumber, new { id = "TelephoneNumber", @class = "form-control phone", placeholder = "Telephone Number" })
                            @Html.ValidationMessageFor(m => m.TelephoneNumber)
                        </div>

                    </div>


                </div>

                <br />
                <div class="row">
                    <div class="col-md-3">

                        <div>
                            <label style="padding-right:6px" for="Password">Password</label>
                            @Html.PasswordFor(m => m.Password, new { id = "Password", @class = "form-control", placeholder = "Password" })
                            @Html.ValidationMessageFor(m => m.Password)
                        </div>

                    </div>

                    <div class="col-md-3">
                        <div>
                            <label style="padding-right:35px" for="ConfirmPassword">Confirm Password</label>
                            @Html.PasswordFor(m => m.ConfirmPassword, new { id = "ConfirmPassword", @class = "form-control", placeholder = "Confirm Password" })
                            @Html.ValidationMessageFor(m => m.ConfirmPassword)
                        </div>

                    </div>

                    <div class="col-md-3">
                        <div class="alert alert-warning"><span class="glyphicon glyphicon-exclamation-sign"></span>&nbsp;Password must have AT LEAST 6 characters including 1 number, 1 uppercase letter and 1 special character</div>
                    </div>

                </div>




            </div>

            <br />
            <br />


        }


        <div>

            <div id="SSCBContainer" class="pull-left" style="padding-top:0px;display:none"><input class="big" type="checkbox" id="StateStaffChkBox"></div>
            <div id="SSLabelContainer" style="padding-top:2px;display:none"><h4 style="font-family: 'Roboto', sans-serif;color:#666">&nbsp;&nbsp;*All Agency/System Access (<span style="color:red">*This is for STATE STAFF ONLY</span>)</h4></div>

        </div>


        <div style="display:none" id="StateStaffContainer">
            <br />

            @if (!Request.IsAuthenticated)
            {




                <div class="row">
                    <div class="col-md-12">

                        <div class="form-group">

                            <h3 style="font-family: 'Roboto', sans-serif;color:#666"> <span class="glyphicon glyphicon-user"></span>&nbsp;Select your Role</h3>

                        </div>
                    </div>
                </div>
                <hr />
                <br />

                <div class="row">

                    <div class="col-md-2">


                        Select the Role you would like for the system


                    </div>

                    <div class="col-md-10">


                        @Html.DropDownList("RoleBinID", null, new { style = "font-size:20px" })
                        @Html.ValidationMessageFor(m => m.RoleBinID)


                    </div>

                </div>
                <br />
                <div id="CoordinatorContainer" style="display:none;">

                    <div class="pull-left" style="padding-top:0px;"><input class="big" type="checkbox" id="CoordinatorChkBox"></div>
                    <div style="padding-top:2px"><h4 style="font-family: 'Roboto', sans-serif;color:#666">&nbsp;&nbsp;Access Coordinator (Will Receive User Access Requests)</h4></div>

                </div>


            }
            else
            {


                if (ViewBag.UserRoleBinID == 3 || ViewBag.UserRoleBinID == 13)
                {





                    <div class="row">
                        <div class="col-md-12">

                            <div class="form-group">

                                <h3 style="font-family: 'Roboto', sans-serif;color:#666"> <span class="glyphicon glyphicon-user"></span>&nbsp;Select your Role</h3>

                            </div>
                        </div>
                    </div>
                    <hr />
                    <br />

                    <div class="row">

