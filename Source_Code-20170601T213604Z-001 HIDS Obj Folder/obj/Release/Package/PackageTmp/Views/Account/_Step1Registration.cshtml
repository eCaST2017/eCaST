                }

                if ($('#LastName').val() == "") {
                    iStopSub = 1;
                    sErr += "-Please enter a last name.\n";
                }

                if ($('#Email').val() == "") {
                    iStopSub = 1;
                    sErr += "-Please enter an email address.\n";
                }

                if ($('#TelephoneNumber').val() == "") {
                    iStopSub = 1;
                    sErr += "-Please enter a telephone number.\n";
                }
                else {

                    var tel = $('#TelephoneNumber').val();
                    var targ = tel.replace(/[^\d]/g, ''); // remove all non-digits
                    if (targ.length != 10) {
                        // targ is a valid phone number

                        iStopSub = 1;
                        sErr += "-Please enter a valid telephone number.\n";

                    }


                }
                if ($('#Password').val() == "") {
                    iStopSub = 1;
                    sErr += "-Please enter a password.\n";
                }
                else {

                    var ps = $('#Password').val();
                    var myRegEx = new RegExp('^(?=.*?[A-Z])(?=.*?[a-z])(?=.*?[0-9])(?=.*?[#?!@@$%^&*-]).{6,}$');

                    if (ps != myRegEx.exec(ps)) {

                        iStopSub = 1;
                        sErr += "-Password must be at least 6 characters long and have at least 1 number, 1 uppercase letter and 1 special character.\n";

                    }


                }
                if ($('#ConfirmPassword').val() == "") {
                    iStopSub = 1;
                    sErr += "-Please enter a confirm password.\n";
                }

                if ($('#Password').val() != $('#ConfirmPassword').val()) {
                    iStopSub = 1;
                    sErr += "-Passwords must match.\n";
                }


                if (iStopSub != 0) {
                    alert(sErr);
                    e.preventDefault();

                }


            });


