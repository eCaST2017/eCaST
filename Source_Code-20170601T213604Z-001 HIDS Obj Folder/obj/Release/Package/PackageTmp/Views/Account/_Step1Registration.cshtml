                            @Html.DropDownList("RoleBinID", null, new { style = "font-size:20px" })
                            @Html.ValidationMessageFor(m => m.RoleBinID)


                        </div>

                    </div>


                    <div id="CoordinatorContainer" style="display:none">

                        <div class="pull-left" style="padding-top:0px;"><input class="big" type="checkbox" id="CoordinatorChkBox"></div>
                        <div style="padding-top:2px"><h4 style="font-family: 'Roboto', sans-serif;color:#666">&nbsp;&nbsp;Access Coordinator (Will Receive User Access Requests)</h4></div>

                    </div>


                }






            }


            <br />
            <div class="row">
                <div class="col-md-12">

                    <div class="form-group">

                        <h3 style="font-family: 'Roboto', sans-serif;color:#666"> <span class="glyphicon glyphicon-play-circle"></span>&nbsp;Select your Program Access</h3>

                    </div>
                </div>
            </div>
            <hr />
            <br />
            <div style="padding-left:12px" class="row">

                <div class="col-md-2">


                    Select the Programs you would like to access



                </div>
                <div class="col-md-4">

                    <div class="input-group btn-group">


                        @Html.DropDownList("testFF", (SelectList)ViewData["OptionListF"], new { @class = "NewCssdropdown", multiple = "", @placeholder = "Click to Add" })


                        <input type="hidden" id="selectedIDsFF" />



                    </div>

                </div>



            </div>




        </div>

        <br />
        @if (!Request.IsAuthenticated)
        {

            <div class="row">

                <div class="col-md-12">


                    <div style="display:block" id="OrgSection">


                        <div class="row">


                            <div class="col-md-12">

                                <h3 style="font-family: 'Roboto', sans-serif;color:#666"> <span class="glyphicon glyphicon-map-marker"></span>&nbsp;Agency/System Access</h3>

                            </div>

                        </div>
                        <hr />
                        <br />

                        <div style="display:block" id="ProgramContainer">

                            <div class="row">

                                <div class="col-md-2">


                                    Select the Agency you would like to access



                                </div>


                                <div class="col-md-10">


                                    @Html.DropDownList("testAS", (SelectList)ViewData["OptionListAS"], new { @class = "NewCssdropdown", multiple = "", @placeholder = "Click to Add" })


                                    <input type="hidden" id="selectedIDsAS" />

                                    <div>
                                        <input type="checkbox" id="checkboxAS">
                                        Select All


                                    </div>
                                    @*@Html.DropDownList("AgencySiteID", null, new { style = "font-size:20px" })
                                        @Html.ValidationMessageFor(m => m.AgencySiteID)*@





                                </div>

                            </div>
                            <br />
                            <br />


                        </div>


                        <div id="ProgramListContainer"></div>

                    </div>



                    <br />

                </div>
            </div>


        }
        else
        {

            if (ViewBag.UserRoleBinID == 2 || ViewBag.UserRoleBinID == 6)
            {


                <div class="row">

                    <div class="col-md-12">


                        <div style="display:block" id="OrgSection">


                            <div class="row">


                                <div class="col-md-12">

                                    <h3 style="font-family: 'Roboto', sans-serif;color:#666"> <span class="glyphicon glyphicon-map-marker"></span>&nbsp;Agency Access</h3>

                                </div>

                            </div>
                            <hr />
                            <br />

                            <div style="display:block" id="ProgramContainer">

                                <div class="row">

                                    <div class="col-md-2">


                                        Select the Agency you would like to access



                                    </div>


                                    <div class="col-md-10">


                                        @Html.DropDownList("testAS", (SelectList)ViewData["OptionListAS"], new { @class = "NewCssdropdown", multiple = "", @placeholder = "Click to Add" })


                                        <input type="hidden" id="selectedIDsAS" />

                                        <div>
                                            <input type="checkbox" id="checkboxAS">
                                            Select All


                                        </div>
                                        @*@Html.DropDownList("AgencySiteID", null, new { style = "font-size:20px" })
                                            @Html.ValidationMessageFor(m => m.AgencySiteID)*@





                                    </div>

                                </div>
                                <br />
                                <br />


                            </div>


                            <div id="ProgramListContainer"></div>

                        </div>



                        <br />

                    </div>
                </div>




            }




        }






        <hr />
        <div style="padding-left:30px">


            <div style="padding-right:30px" align="right">



                @if (!Request.IsAuthenticated)
                {

                    <a style="text-decoration:none" href='@Url.Action("Index", new { Controller = "Home" })'>


                        <span class="btn btn-primary">Return to Home</span>

                    </a>


                }
                else
