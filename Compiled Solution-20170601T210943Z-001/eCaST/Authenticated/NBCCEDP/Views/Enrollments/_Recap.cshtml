@model eCaST.ViewModels.EnrollmentViewModel

@{
    ViewBag.Title = "Enrollment Details";
}



<div class="btn-group" role="group" aria-label="...">
    <button style="background-color:#F5F5F5;" id="EnrollmentEdit0Button" type="button" class="btn btn-default"><i class="fa fa-pencil" aria-hidden="true"></i></button>
    
    @if (ViewBag.RoleBinID == 1 || ViewBag.RoleBinID == 3 || ViewBag.RoleBinID == 13)
    {


        <button style="background-color:#F5F5F5;" id="EnrollmentConfigButton" type="button" class="btn btn-default"><i class="fa fa-cog" aria-hidden="true"></i></button>


    }
    
    
    
    <button style="background-color:#F5F5F5;cursor:text" type="button" class="btn btn-default"><i class="fa fa-calendar-o" aria-hidden="true"></i>&nbsp; Enrollment</button>
    <button style="cursor:text" type="button" class="btn btn-default"><i class="fa fa-calendar-o" aria-hidden="true"></i>&nbsp;@Html.FormatValue(Model.EnrollmentDate, "{0:MM/dd/yyyy}")</button>
    <button style="cursor:text" type="button" class="btn btn-default">@Model.InsuranceBinName</button>
    <button style="cursor:text" type="button" class="btn btn-default">@Model.MedicareBinName</button>
    
    @if (@Model.UninsuredClientReferredBinName != null)
    {


        <button style="cursor:text" type="button" class="btn btn-default">Uninsured Referred: @Model.UninsuredClientReferredBinName</button>


    }


    @*<button type="button" class="btn btn-default"><span class="glyphicon glyphicon-usd"></span>&nbsp;@Model.MonthlyIncome</button>
    <button type="button" class="btn btn-default"><i class="fa fa-users" aria-hidden="true"></i>&nbsp;@Model.NumberLivingOnIncome</button>*@
    <button style="cursor:text" type="button" class="btn btn-default">@Model.PercentPoverty &nbsp;<i class="fa fa-percent" aria-hidden="true"></i>&nbsp;&nbsp;FPL</button>
    @if (ViewBag.Today >= Model.ExpirationDate)
    {


        <button style="cursor:text;color:#ff0000" type="button" class="btn btn-default btn-xl"><i class="fa fa-exclamation-triangle" aria-hidden="true"></i>&nbsp;&nbsp;Expired</button>

    }
    else
    {

        <button style="cursor:text;color:#52a55d" type="button" class="btn btn-default btn-xl"><i class="fa fa-check-circle" aria-hidden="true"></i>&nbsp;&nbsp;Active</button>

    }


</div>




