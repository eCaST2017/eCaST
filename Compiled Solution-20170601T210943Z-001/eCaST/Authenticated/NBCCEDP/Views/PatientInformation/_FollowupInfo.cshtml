@model eCaST.ViewModels.SupportSessionViewModel



<div align="right">



    <div class="alert alert-warning">


        @if (ViewBag.HBSessionCompletionTypeBinID == 5 || ViewBag.HBSessionCompletionTypeBinID == 6)
        {

            <label>DPP/HC Support Session Completion Status:</label><text>&nbsp;&nbsp;</text><label style="color:#f00">@ViewBag.HBSessionCompletionTypeBinName</label>

        }
        else
        {

            if (@ViewBag.FollowupCompletenessBinID == 1)
            {

                <label>Followup Completion Status:</label><text>&nbsp;&nbsp;</text><label style="color:#68ae2a">@ViewBag.FollowupCompletenessBinName</label>


            }

            else
            {

                <label>Followup Completion Status:</label><text>&nbsp;&nbsp;</text><label style="color:#f00">@ViewBag.FollowupCompletenessBinName</label>
               

            }


        }

        @if (ViewBag.FollowupCheck == 4 || ViewBag.FollowupCheck == 5 || ViewBag.FollowupCheck == 6)
        {

            // Do Nothing

        }
        else
        {

            if (@ViewBag.FollowupCompletenessBinID == 2 || @ViewBag.FollowupCompletenessBinID == 3)
            {

                <button id="EditFUButton" type="submit" class="btn btn-success">Followup Assessment</button>


            }

        }

        <br />

    </div>





</div>



<script type="text/javascript">



    $(function () {

        $("#EditFUButton").click(function (evt) {
            evt.preventDefault();
            $('#AccordionMasterContainer').load('@Url.Action("_FollowAssessment", "PatientInformation", new { id = ViewBag.ScreeningID })');



        });

     });



</script>
