@model IEnumerable<eCaST.ViewModels.PatientInfoViewModel>

@*<link href="@Url.Content("~/Content/bootstrap.custom.css")" rel="stylesheet" type="text/css" />*@
<link href="@Url.Content("~/Content/slider.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("~/Content/themes/base/jquery.ui.slider.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("~/Scripts/app/patientinfo.js")" type="text/javascript"></script>
@*<script src="@Url.Content("~/Scripts/bootstrap.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/bootstrap.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/bootstrap-multiselect.js")" type="text/javascript"></script>
    <link href="@Url.Content("~/Content/bootstrap-multiselect.css")" rel="stylesheet" type="text/css" />*@

@if (ViewBag.FollowupCheck == 1)
{

    if (ViewBag.FollowCompCheck != 1)
    {


        <div id="FollowupSectionInline"></div>

    }

}


@foreach (var item in Model)
{



    <div class="alert alert-default breadcrumb">



        <div class="row">

            <div style="padding-left:22px"> <label style="color:#68ae2a">Completed on @(((DateTime)ViewBag.AssessmentDate).ToString("MM/dd/yyyy"))</label></div>
            <div align="right" class="col-md-12">
                @if (ViewBag.BPPlusCheck == true)
                {
                    <text>This is a</text> <img id="BBPPlusIcon" src="@Url.Content(String.Format("~/Images/CoverageIconBPPLUS.png"))" /> <text>Screening. Click Edit to Update or Upgrade.</text>

                }
                <button id="EditPatientInfoButton" class="btn btn-default btn-xs">  <span class="glyphicon glyphicon-pencil"></span> Edit</button>
            </div>

        </div>

        <div class="row">
            <div class="col-md-12">
                <h3 style="font-family:'Lato', sans-serif; color:#666; font-weight:300"><i style="color:#666" class="fa fa-heartbeat"></i>&nbsp;Initial Cardio Health Assessment</h3>
            </div>



        </div>





        <hr class="style-two" />



        <div class="alert alert-success">
            <div class="row">

                <div class="col-md-12"><span style="font-size:medium;color:#fff; width:100%" class="badge">Demographics</span></div>


            </div>

            <hr class="style-two" />
            <div align="left">Education: <b>@item.EducationBinName</b></div>
            <div align="left">Primary Language: <b>@item.LanguageBinName</b></div>

        </div>




        <div class="alert alert-@ViewBag.CholAlert">

            <div class="row">

                <div class="col-md-12"><span style="font-size:medium;color:#fff; width:100%" class="badge">Cholesterol</span></div>


            </div>
            <hr class="style-two" />
            <div align="left">High cholesterol?: <b>@ViewBag.CholCheck</b></div>


            @{if (ViewBag.CholCheckID == 1)
            {


                <div align="left">Taking Medication?: <b>@ViewBag.CholMedCheck</b></div>


                if (ViewBag.CholMedCheck == "Yes")
                {

                    <div align="left">Days a week?: <b>@ViewBag.CholMedDaysCheck</b></div>

                }



            }

            }

        </div>



        <div class="alert alert-@ViewBag.HyperAlert">

            <div class="row">

                <div class="col-md-12"><span style="font-size:medium;color:#fff; width:100%" class="badge">Hypertension</span></div>


            </div>

            <hr class="style-two" />

            <div align="left">High blood pressure?: <b>@ViewBag.HyperCheck</b></div>


            @{if (ViewBag.HyperCheckID == 1)
            {


                <div align="left">Taking Medication?: <b>@ViewBag.HyperMedCheck</b></div>


                if (ViewBag.HyperMedCheck == "Yes")
                {

                    <div align="left">Days a week?: <b>@ViewBag.HyperMedDaysCheck</b></div>

                }



                if (ViewBag.HyperMeasureCheck == "Yes")
                {

                    <div align="left">Do they measure blood pressure?: <b>@ViewBag.HyperMeasureCheck</b></div>

                }

                if (ViewBag.HyperMeasureYesCheck != "Not Applicable" || ViewBag.HyperMeasureYesCheck != "Don't know/Not sure" || ViewBag.HyperMeasureYesCheck != "Don't want to answer" || ViewBag.HyperMeasureYesCheck != "No answer recorded")
                {

                    <div align="left">How often?: <b>@ViewBag.HyperMeasureYesCheck</b></div>

                }

                if (ViewBag.HyperShareCheck == "Yes")
                {

                    <div align="left">Do they share blood pressure reading with a provider?: <b>@ViewBag.HyperShareCheck</b></div>

                }



            }

            }




        </div>



        <div class="alert alert-@ViewBag.DiaAlert">

            <div class="row">

                <div class="col-md-12"><span style="font-size:medium;color:#fff; width:100%" class="badge">Diabetes</span></div>


            </div>


            <hr class="style-two" />


            <div align="left">Diabetes?: <b>@ViewBag.DiaCheck</b></div>


            @{if (ViewBag.DiaCheckID == 1)
            {


                <div align="left">Taking Medication?: <b>@ViewBag.DiaMedCheck</b></div>


                if (ViewBag.DiaMedCheck == "Yes")
                {

                    <div align="left">Days a week?: <b>@ViewBag.DiaMedDaysCheck</b></div>

                }



            }

            }




        </div>



        <div class="alert alert-@ViewBag.HeartAlert">

            <div class="row">

                <div class="col-md-12"><span style="font-size:medium;color:#fff; width:100%" class="badge">Heart Attack</span></div>


            </div>


            <hr class="style-two" />


            <div align="left">History of Heart Attack?: <b>@ViewBag.HeartCheck</b></div>

        </div>


        <div class="alert alert-@ViewBag.HealthyAlert">


            <div class="row">

                <div class="col-md-12"><span style="font-size:medium;color:#fff; width:100%" class="badge">Diet/Phys. Activity</span></div>


            </div>



            <hr class="style-two" />


            <div align="left">Cups of fruit per day?: <b>@ViewBag.FruitCheck</b></div>

            <div align="left">Cups of vegetables per day?: <b>@ViewBag.VegCheck</b></div>

            <div align="left">2 servings or more of fish weekly?: <b>@ViewBag.FishCheck</b></div>

            <div align="left">3 ounces or more of whole grains daily?: <b>@ViewBag.GrainCheck</b></div>

            <div align="left">Drink less than 36 oz. of sugar added beverages weekly?: <b>@ViewBag.DrinkCheck</b></div>

            <div align="left">Watching sodium intake?: <b>@ViewBag.SodiumCheck</b></div>

            <div align="left">Eating 1,500 mg of sodium or less daily? <b>@ViewBag.SodiumLevelCheck</b></div>

            <div align="left">Minutes of moderate activity per week?: <b>@ViewBag.ModPhysicalCheck</b></div>

            <div align="left">Minutes of vigorous activity per week?: <b>@ViewBag.VigPhysicalCheck</b></div>

        </div>


        <div class="alert alert-@ViewBag.SmokeAlert">

            <div class="row">

                <div class="col-md-12"><span style="font-size:medium;color:#fff; width:100%" class="badge">Smoking Status</span></div>


            </div>



            <hr class="style-two" />


            <div align="left">Do they smoke?: <b>@ViewBag.SmokeCheck</b></div>


            @if (ViewBag.SecondSmokeCheck < 25)
            {


                <div align="left">Hours around secondhand smoke?: <b>@ViewBag.SecondSmokeCheck</b></div>

            }
            else if (ViewBag.SecondSmokeCheck == 66)
            {

                <div align="left">Hours around secondhand smoke?: <b>Less than one hour</b></div>


            }
            else if (ViewBag.SecondSmokeCheck == 88)
            {


                <div align="left">Hours around secondhand smoke?: <b>Don't want to answer</b></div>

            }
            else if (ViewBag.SecondSmokeCheck == 99)
            {


                <div align="left">Hours around secondhand smoke?: <b>No answer recorded</b></div>

            }

        </div>



        <div class="alert alert-info">


            <div class="row">

                <div class="col-md-12"><span style="font-size:medium;color:#fff; width:100%" class="badge">Quality of Life</span></div>


            </div>

            <hr class="style-two" />


            @if (ViewBag.PhysicalHealthCheck < 31)
            {



                <div align="left">Bad physical health days in last 30 days?: <b>@ViewBag.PhysicalHealthCheck</b></div>


            }
            else if (ViewBag.PhysicalHealthCheck == 77)
            {

                <div align="left">Bad physical health days in last 30 days?: <b>Don't know/Not Sure</b></div>


            }
            else if (ViewBag.PhysicalHealthCheck == 88)
            {


                <div align="left">Bad physical health days in last 30 days?: <b>Don't want to answer</b></div>

            }
            else if (ViewBag.PhysicalHealthCheck == 99)
            {


                <div align="left">Bad physical health days in last 30 days?: <b>No answer recorded</b></div>

            }

            @*  <div align="left">Bad mental health days in last 30 days?: <b>@ViewBag.MentalHealthCheck</b></div>*@

            @if (ViewBag.MentalHealthCheck < 31)
            {



                <div align="left">Bad mental health days in last 30 days?: <b>@ViewBag.MentalHealthCheck</b></div>


            }
            else if (ViewBag.MentalHealthCheck == 77)
            {

                <div align="left">Bad mental health days in last 30 days?: <b>Don't know/Not Sure</b></div>


            }
            else if (ViewBag.MentalHealthCheck == 88)
            {


                <div align="left">Bad mental health days in last 30 days?: <b>Don't want to answer</b></div>

            }
            else if (ViewBag.MentalHealthCheck == 99)
            {


                <div align="left">Bad mental health days in last 30 days?: <b>No answer recorded</b></div>

            }


            @*  <div align="left">Bad mental/physical health days in last 30 days?: <b>@ViewBag.MentalPhysicalHealthCheck</b></div>*@

            @if (ViewBag.MentalPhysicalHealthCheck < 31)
            {



                <div align="left">Bad mental/physical health days in last 30 days?: <b>@ViewBag.MentalPhysicalHealthCheck</b></div>


            }
            else if (ViewBag.MentalPhysicalHealthCheck == 77)
            {

                <div align="left">Bad mental/physical health days in last 30 days?: <b>Don't know/Not Sure</b></div>


            }
            else if (ViewBag.MentalPhysicalHealthCheck == 88)
            {


                <div align="left">Bad mental/physical health days in last 30 days?: <b>Don't want to answer</b></div>

            }
            else if (ViewBag.MentalPhysicalHealthCheck == 99)
            {


                <div align="left">Bad mental/physical health days in last 30 days?: <b>No answer recorded</b></div>

            }




        </div>



    </div>



}



<script type="text/javascript">


    $(function () {

        $("#EditPatientInfoButton").click(function (evt) {
            evt.preventDefault();

            $('#AccordionMasterContainer').load('@Url.Action("_PatientInfoEdit", "PatientInformation", new { id = ViewBag.ScreeningID, id2 = ViewBag.AssessmentID })');
         


        });

     });



</script>

<script type="text/javascript">

    $(document).ready(function () {



        $('#FollowupSectionInline').load('@Url.Action("_FollowupInfo", "PatientInformation", new { id = ViewBag.ScreeningID })');


    });

</script>
